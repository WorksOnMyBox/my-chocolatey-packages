<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>B4J</id>
    <version>7.80</version>
    <packageSourceUrl>https://github.com/elpatron68/my-chocolatey-packages</packageSourceUrl>
    <owners>elpatron</owners>
    <title>B4J (Install)</title>
    <authors>Anywhere Software, Erel Uziel</authors>
    <projectUrl>https://www.b4x.com/</projectUrl>
